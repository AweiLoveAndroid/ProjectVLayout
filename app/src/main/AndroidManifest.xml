<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.youngport.app.projectvlayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".VLayout.ColumnLayoutHelperActivity"></activity>
        <activity android:name=".VLayout.FixLayoutHelperActivity"></activity>
        <activity android:name=".VLayout.FloatLayoutHelperActivity"></activity>
        <activity android:name=".VLayout.OnePlusNLayoutHelperActivity"></activity>
        <activity android:name=".VLayout.GridLayoutHelperActivity"></activity>
        <activity android:name=".VLayout.LinearLayoutHelperActivity"></activity>
        <activity android:name=".VLayout.ScrollFixLayoutHelperActivity"></activity>
        <activity android:name=".VLayout.StickyLayoutHelperActivity"></activity>
        <activity android:name=".VLayout.StaggeredGridLayoutHelperActivity"></activity>
        <activity android:name=".VLayout.SingleLayoutHelperActivity"></activity>
        <activity android:name=".VLayout.AllActivity"></activity>

    </application>

</manifest>